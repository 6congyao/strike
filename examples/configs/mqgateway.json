{
  "servers":[
    {
      "default_log_path":"stdout",
      "use_edge_mode": false,
      "listeners":[
        {
          "name":"httpListener",
          "address": ":8045",
          "bind_port": true,
          "log_path": "stdout",
          "filter_chains": [{
            "tls_context":{
            },
            "filters": [
              {
                "type": "proxy",
                "config": {
                  "downstream_protocol": "Http1",
                  "upstream_protocol": "MQ",
                  "router_config_name":"client_router"
                }
              },
              {
                "type":"connection_manager",
                "config":{
                  "router_config_name":"client_router",
                  "virtual_hosts":[{
                    "name":"clientHost",
                    "domains": ["*"],
                    "routers": [
                      {
                        "match":{"headers":[{"name":"service","value":".*"}]},
                        "route":{"cluster_name":"clientCluster"}
                      }
                    ]
                  }]
                }
              }]
          }],
          "stream_filters": [
            {
              "type": "common",
              "config": {
                "rule_configs": [
                  {
                    "enable": true,
                    "id": 1,
                    "index": 1,
                    "limit": {
                      "limit_strategy": "QPS",
                      "max_burst_ratio": 1.0,
                      "period_ms": 100,
                      "max_allows": 0
                    },
                    "name": "RuleAllowPost",
                    "resources": [
                      {
                        "headers": [
                          {
                            "compare_type": "NOTEQUALS",
                            "key": "x-strike-method",
                            "value": "POST"
                          }
                        ]
                      }
                    ],
                    "run_mode": "CONTROL"
                  }
                ]
              }
            }
          ]
        },
